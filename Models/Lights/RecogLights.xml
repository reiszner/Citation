<?xml version='1.0' encoding='utf-8'?>

<PropertyList>

    <path>RecogLights.ac</path>

    <animation>
        <type>select</type>
        <object-name>RecogLightCone_L</object-name>
        <condition>
            <property>systems/electrical/outputs/recog-light[0]</property>
        </condition>
    </animation>

	<animation>
		<type>light</type>
		<light-type>spot</light-type>
		<name>RecogLightLeft.light</name>
		<object-name>RecogLightCone_L</object-name>
		<nopreview/>
		<condition>
			<property>systems/electrical/outputs/recog-light[0]</property>
		</condition>
		<position>
			<x>6.7</x>
			<y>-7.5</y>
			<z>0</z>
		</position>
		<direction>
			<x>-29</x>
			<y>-3.76</y>
			<z>-3.6</z>
		</direction>
		<ambient>
			<r>0.9</r>
			<g>0.8</g>
			<b>0.6</b>
			<a>0.9</a>
		</ambient>
		<diffuse>
			<r>0.7</r>
			<g>0.6</g>
			<b>0.4</b>
			<a>0.7</a>
		</diffuse>
		<specular>
			<r>0.7</r>
			<g>0.6</g>
			<b>0.4</b>
			<a>0.7</a>
		</specular>
		<dim-factor>
			<property>systems/electrical/outputs/recog-light[0]</property>
			<factor>0.042</factor>
			<offset>0</offset>
			<min>0</min>
			<max>1</max>
		</dim-factor>
		<attenuation>
			<c>2.0</c>
			<l>0.05</l>
			<q>0.005</q>
		</attenuation>
		<exponent>30.0</exponent>
		<cutoff>300</cutoff>
		<near-m>0.01</near-m>
		<far-m>300</far-m>
	</animation>

    <animation>
        <type>select</type>
        <object-name>RecogLightCone_R</object-name>
        <condition>
            <property>systems/electrical/outputs/recog-light[1]</property>
        </condition>
    </animation>

	<animation>
		<type>light</type>
		<light-type>spot</light-type>
		<name>RecogLightRight.light</name>
		<object-name>RecogLightCone_R</object-name>
		<nopreview/>
		<condition>
			<property>systems/electrical/outputs/recog-light[1]</property>
		</condition>
		<position>
			<x>6.7</x>
			<y>7.5</y>
			<z>0</z>
		</position>
		<direction>
			<x>-29</x>
			<y>3.76</y>
			<z>-3.6</z>
		</direction>
		<ambient>
			<r>0.9</r>
			<g>0.8</g>
			<b>0.6</b>
			<a>0.9</a>
		</ambient>
		<diffuse>
			<r>0.7</r>
			<g>0.6</g>
			<b>0.4</b>
			<a>0.7</a>
		</diffuse>
		<specular>
			<r>0.7</r>
			<g>0.6</g>
			<b>0.4</b>
			<a>0.7</a>
		</specular>
		<dim-factor>
			<property>systems/electrical/outputs/recog-light[1]</property>
			<factor>0.042</factor>
			<offset>0</offset>
			<min>0</min>
			<max>1</max>
		</dim-factor>
		<attenuation>
			<c>2.0</c>
			<l>0.05</l>
			<q>0.005</q>
		</attenuation>
		<exponent>30.0</exponent>
		<cutoff>300</cutoff>
		<near-m>0.01</near-m>
		<far-m>300</far-m>
	</animation>

</PropertyList>
