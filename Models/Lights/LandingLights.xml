<?xml version='1.0' encoding='utf-8'?>

<PropertyList>

  <path>LandingLights.ac</path>

  <animation>
    <type>select</type>
    <object-name>LandingLightCone_L</object-name>
    <condition>
      <and>
        <property>systems/electrical/outputs/landing-light[0]</property>
        <greater-than>
          <property>gear/gear[1]/position-norm</property>
          <value>0.97</value>
        </greater-than>
      </and>
    </condition>
  </animation>

  <animation>
    <type>light</type>
    <light-type>spot</light-type>
    <name>LandingLightLeft.light</name>
    <object-name>LandingLightCone_L</object-name>
    <nopreview/>
    <condition>
      <property>systems/electrical/outputs/landing-light[0]</property>
    </condition>
    <position>
      <x>7.015</x>
      <y>-2.40</y>
      <z>-0.01</z>
    </position>
    <direction>
      <x>-29</x>
      <y>-3.76</y>
      <z>-3.6</z>
    </direction>
    <ambient>
      <r>0.9</r>
      <g>0.8</g>
      <b>0.6</b>
      <a>1.0</a>
    </ambient>
    <diffuse>
      <r>0.9</r>
      <g>0.8</g>
      <b>0.6</b>
      <a>0.7</a>
    </diffuse>
    <specular>
      <r>0.9</r>
      <g>0.8</g>
      <b>0.6</b>
      <a>0.7</a>
    </specular>
    <dim-factor>
      <property>systems/electrical/outputs/landing-light[0]</property>
      <factor>0.042</factor>
      <offset>0</offset>
      <min>0</min>
      <max>1</max>
    </dim-factor>
    <attenuation>
      <c>2.0</c>
      <l>0.002</l>
      <q>0.0025</q>
    </attenuation>
    <exponent>30.0</exponent>
    <cutoff>300</cutoff>
    <near-m>0.01</near-m>
    <far-m>300</far-m>
  </animation>

  <animation>
    <type>select</type>
    <object-name>LandingLightCone_R</object-name>
    <condition>
      <and>
        <property>systems/electrical/outputs/landing-light[1]</property>
        <greater-than>
          <property>gear/gear[2]/position-norm</property>
          <value>0.97</value>
        </greater-than>
      </and>
    </condition>
  </animation>

  <animation>
    <type>light</type>
    <light-type>spot</light-type>
    <name>LandingLightRight.light</name>
    <object-name>LandingLightCone_R</object-name>
    <nopreview/>
    <condition>
      <property>systems/electrical/outputs/landing-light[1]</property>
    </condition>
    <position>
      <x>7.015</x>
      <y>2.40</y>
      <z>-0.01</z>
    </position>
    <direction>
      <x>-29</x>
      <y>3.76</y>
      <z>-3.6</z>
    </direction>
    <ambient>
      <r>0.9</r>
      <g>0.8</g>
      <b>0.6</b>
      <a>1.0</a>
    </ambient>
    <diffuse>
      <r>0.9</r>
      <g>0.8</g>
      <b>0.6</b>
      <a>0.7</a>
    </diffuse>
    <specular>
      <r>0.9</r>
      <g>0.8</g>
      <b>0.6</b>
      <a>0.7</a>
    </specular>
    <dim-factor>
      <property>systems/electrical/outputs/landing-light[1]</property>
      <factor>0.042</factor>
      <offset>0</offset>
      <min>0</min>
      <max>1</max>
    </dim-factor>
    <attenuation>
      <c>2.0</c>
      <l>0.002</l>
      <q>0.0025</q>
    </attenuation>
    <exponent>30.0</exponent>
    <cutoff>300</cutoff>
    <near-m>0.01</near-m>
    <far-m>300</far-m>
  </animation>

</PropertyList>
