<?xml version='1.0' encoding='utf-8'?>

<PropertyList>

    <path>StrobeLights.ac</path>

    <animation>
        <type>select</type>
        <object-name>sphereStrobeLight.left</object-name>
        <object-name>sphereStrobeLight.right</object-name>
        <condition>
            <property>systems/electrical/outputs/strobe</property>
        </condition>
    </animation>

    <animation>
        <type>light</type>
        <light-type>point</light-type>
        <name>StrobeLightLeft.light</name>
        <object-name>sphereStrobeLight.left</object-name>
        <nopreview/>
        <condition>
            <property>systems/electrical/outputs/strobe</property>
        </condition>
        <position>
            <x>6.92</x>
            <y>-7.82</y>
            <z>0.407</z>
        </position>
        <direction>
            <x>0.0</x>
            <y>-1.0</y>
            <z>0.0</z>
        </direction>
        <ambient>
            <r>1</r>
            <g>1</g>
            <b>1</b>
            <a>1</a>
        </ambient>
        <diffuse>
            <r>0.6</r>
            <g>0.6</g>
            <b>0.55</b>
            <a>1</a>
        </diffuse>
        <specular>
            <r>0.6</r>
            <g>0.6</g>
            <b>0.55</b>
            <a>1</a>
        </specular>
        <dim-factor>
            <property>systems/electrical/outputs/strobe</property>
            <factor>0.042</factor>
            <offset>0</offset>
            <min>0</min>
            <max>1</max>
        </dim-factor>
        <attenuation>
            <c>3.0</c>
            <l>2</l>
            <q>4</q>
        </attenuation>
        <near-m>0.1</near-m>
        <far-m>50</far-m>
    </animation>

    <animation>
        <type>light</type>
        <light-type>point</light-type>
        <name>StrobeLightRight.light</name>
        <object-name>sphereStrobeLight.right</object-name>
        <nopreview/>
        <condition>
            <property>systems/electrical/outputs/strobe</property>
        </condition>
        <position>
            <x>6.92</x>
            <y>7.82</y>
            <z>0.407</z>
        </position>
        <direction>
            <x>0.0</x>
            <y>1.0</y>
            <z>0.0</z>
        </direction>
        <ambient>
            <r>1</r>
            <g>1</g>
            <b>1</b>
            <a>1</a>
        </ambient>
        <diffuse>
            <r>0.6</r>
            <g>0.6</g>
            <b>0.55</b>
            <a>1</a>
        </diffuse>
        <specular>
            <r>0.6</r>
            <g>0.6</g>
            <b>0.55</b>
            <a>1</a>
        </specular>
        <dim-factor>
            <property>systems/electrical/outputs/strobe</property>
            <factor>0.042</factor>
            <offset>0</offset>
            <min>0</min>
            <max>1</max>
        </dim-factor>
        <attenuation>
            <c>3.0</c>
            <l>2</l>
            <q>4</q>
        </attenuation>
        <near-m>0.1</near-m>
        <far-m>50</far-m>
    </animation>

