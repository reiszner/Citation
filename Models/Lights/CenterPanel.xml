<?xml version="1.0"?>

<PropertyList>

    <path>CenterPanel.ac</path>

    <animation>
        <type>select</type>
        <object-name>IntLight.CenterPanel.Cone</object-name>
        <condition>
          <and>
            <property>systems/electrical/outputs/main-right-xover/light-panel-center</property>
            <greater-than>
              <property>controls/lighting/center-panel-light-emission</property>
              <value>0.01</value>
            </greater-than>
          </and>
        </condition>
    </animation>

    <animation>
	<type>light</type>
	<light-type>spot</light-type>
	<name>CenterPanelLight</name>
	<object-name>IntLight.CenterPanel.Cone</object-name>
	<nopreview/>
	<position>
		<x>2.49712</x>
		<y>0.0</y>
		<z>0.77277</z>
	</position>
	<direction>
		<x>-0.1</x>
		<y>0.0</y>
		<z>-2</z>
	</direction>
	<ambient>
		<r>0.9</r>
		<g>0.7</g>
		<b>0.5</b>
		<a>1</a>
	</ambient>
	<diffuse>
		<r>0.7</r>
		<g>0.4</g>
		<b>0.3</b>
		<a>1</a>
	</diffuse>
	<specular>
		<r>0.7</r>
		<g>0.4</g>
		<b>0.3</b>
		<a>1</a>
	</specular>
	<dim-factor>
		<property>controls/lighting/center-panel-light-emission</property>
		<factor>1</factor>
		<offset>0</offset>
		<min>0</min>
		<max>1</max>
	</dim-factor>
	<attenuation>
		<c>0.0</c>
		<l>3</l>
		<q>50</q>
	</attenuation>
	<cutoff>70</cutoff>
	<near-m>0.001</near-m>
	<far-m>2</far-m>
    </animation>

</PropertyList>
