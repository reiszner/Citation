  <!-- The cup-holders serve as magic buttons to lower (stow) or raise
       (deploy) the tables. All cup-holders act as one object, so act
       on all tables that are in the vertical position. -->

  <animation>
    <type>pick</type>
    <object-name>Cabin.Cupholders</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <binding>
        <condition>
          <greater-than>
            <property>controls/table[1]/out-position-deg</property>
            <value>89</value>
          </greater-than>
        </condition>
        <command>property-adjust</command>
        <property>controls/table[1]/slide-position-m</property>
        <step>0.01</step>
        <min>0</min>
        <max>0.243979</max>
        <wrap>false></wrap>
      </binding>
      <binding>
        <condition>
          <greater-than>
            <property>controls/table[2]/out-position-deg</property>
            <value>89</value>
          </greater-than>
        </condition>
        <command>property-adjust</command>
        <property>controls/table[2]/slide-position-m</property>
        <step>0.01</step>
        <min>0</min>
        <max>0.243979</max>
        <wrap>false></wrap>
      </binding>
      <binding>
        <condition>
          <greater-than>
            <property>controls/table[3]/out-position-deg</property>
            <value>89</value>
          </greater-than>
        </condition>
        <command>property-adjust</command>
        <property>controls/table[3]/slide-position-m</property>
        <step>0.01</step>
        <min>0</min>
        <max>0.243979</max>
        <wrap>false></wrap>
      </binding>
    </action>
    <action>
      <button>1</button>
      <repeatable>true</repeatable>
      <binding>
        <condition>
          <greater-than>
            <property>controls/table[1]/out-position-deg</property>
            <value>89</value>
          </greater-than>
        </condition>
        <command>property-adjust</command>
        <property>controls/table[1]/slide-position-m</property>
        <step>-0.01</step>
        <min>0</min>
        <max>0.243979</max>
        <wrap>false></wrap>
      </binding>
      <binding>
        <condition>
          <greater-than>
            <property>controls/table[2]/out-position-deg</property>
            <value>89</value>
          </greater-than>
        </condition>
        <command>property-adjust</command>
        <property>controls/table[2]/slide-position-m</property>
        <step>-0.01</step>
        <min>0</min>
        <max>0.243979</max>
        <wrap>false></wrap>
      </binding>
      <binding>
        <condition>
          <greater-than>
            <property>controls/table[3]/out-position-deg</property>
            <value>89</value>
          </greater-than>
        </condition>
        <command>property-adjust</command>
        <property>controls/table[3]/slide-position-m</property>
        <step>-0.01</step>
        <min>0</min>
        <max>0.243979</max>
        <wrap>false></wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>material</type>
    <condition>
      <property>systems/electrical/outputs/main-left/light-indirect</property>
      <greater-than>
        <property>controls/lighting/cabin-lights-norm</property>
	<value>0.05</value>
      </greater-than>
    </condition>
    <object-name>Cabin</object-name>
    <object-name>Cabin.AftComp.Door</object-name>
    <object-name>Cabin.Cupholders</object-name>
    <object-name>Cabin.DoorFrame</object-name>
    <object-name>Cabin.Floor</object-name>
    <object-name>Cabin.FllorAirVents</object-name>
    <object-name>Cabin.OHPanel</object-name>
    <object-name>Cabin.door1</object-name>
    <object-name>Ladder</object-name>
    <object-name>Ladder.Anchors</object-name>
    <object-name>RfrshmntCntr</object-name>
    <object-name>RfrshmntCntr.handles</object-name>
    <object-name>Seat.1</object-name>
    <object-name>Seat.2</object-name>
    <object-name>Seat.3</object-name>
    <object-name>Seat.4</object-name>
    <object-name>Seat.5</object-name>
    <object-name>Seat.6</object-name>
    <object-name>Seat.7</object-name>
    <object-name>Seat.levers</object-name>
    <object-name>Seat.mountLedges</object-name>
    <object-name>Table.1.in</object-name>
    <object-name>Table.1.out</object-name>
    <object-name>Table.2.in</object-name>
    <object-name>Table.2.out</object-name>
    <object-name>Table.3.in</object-name>
    <object-name>Table.3.out</object-name>
    <object-name>TableCasings</object-name>
    <object-name>Cabin.MapLights</object-name>
    <object-name>Cabin.LightKnob</object-name>
    <object-name>Cabin.LightKnob.Plate</object-name>
    <emission>
      <red>0.3</red>
      <green>0.2</green>
      <blue>0.16</blue>
      <factor-prop>controls/lighting/cabin-lights-norm</factor-prop>
    </emission>
  </animation>

  <!-- Cabin Map Lights knob and animations -->
  <animation>
    <type>knob</type>
    <object-name>Cabin.LightKnob</object-name>
    <property>controls/lighting/cabin-lights-norm</property>
    <factor>135</factor>
    <center>
      <x-m>4.27121</x-m>
      <y-m>-0.57671</y-m>
      <z-m>0.85835</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <command>property-adjust</command>
        <property>controls/lighting/cabin-lights-norm</property>
        <factor>0.1</factor>
        <min>0</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <hovered>
      <binding>
        <command>set-tooltip</command>
        <tooltip-id>cabin-lights</tooltip-id>
        <label>Cabin lights: %3d%%</label>
        <mapping>percent</mapping>
        <property>controls/lighting/cabin-lights-norm</property>
      </binding>
    </hovered>
  </animation>
  <animation>
    <type>pick</type>
    <object-name>Cabin.LightKnob.Plate</object-name>
    <property>controls/lighting/cabin-lights-norm</property>
    <factor>135</factor>
    <action>
      <button>0</button>
      <button>4</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>controls/lighting/cabin-lights-norm</property>
        <step>0.1</step>
        <min>0</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <action>
      <button>1</button>
      <button>3</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>controls/lighting/cabin-lights-norm</property>
        <step>-0.1</step>
        <min>0</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <hovered>
      <binding>
        <command>set-tooltip</command>
        <tooltip-id>cabin-lights</tooltip-id>
        <label>Cabin lights: %3d%%</label>
        <mapping>percent</mapping>
        <property>controls/lighting/cabin-lights-norm</property>
      </binding>
    </hovered>
  </animation>

  <animation>
    <type>material</type>
    <condition>
      <property>systems/electrical/outputs/main-right/light-cabin</property>
      <greater-than>
        <property>controls/lighting/cabin-lights-norm</property>
        <value>0.35</value>
      </greater-than>
    </condition>
    <object-name>Cabin.MapLights.Bulb</object-name>
    <emission>
      <red>1</red>
      <green>1</green>
      <blue>1</blue>
      <factor-prop>controls/lighting/cabin-lights-norm</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <condition>
      <property>systems/electrical/outputs/main-right/light-cabin</property>
      <greater-than>
        <property>controls/lighting/cabin-lights-norm</property>
	<value>0.35</value>
      </greater-than>
    </condition>
    <object-name>Cabin.MapLights.ins</object-name>
    <emission>
      <red>1</red>
      <green>1</green>
      <blue>1</blue>
      <factor-prop>controls/lighting/cabin-lights-norm</factor-prop>
    </emission>
  </animation>

  <!-- Chrome Effect for interior parts -->
  <effect>
    <inherits-from>Aircraft/Citation/Models/Effects/chrome-flightdeck</inherits-from>
    <object-name>Ladder.lRope</object-name>
    <object-name>Ladder.rRope</object-name>
    <object-name>RfrshmntCntr.handles</object-name>
    <object-name>Cabin.FloorAirVents</object-name>
    <object-name>Cabin.Cupholders</object-name>
    <object-name>Seat.levers</object-name>
    <object-name>Seats.mountLedges</object-name>
    <object-name>Cabin.MapLights</object-name>
    <object-name>Cabin.MapLights.ins</object-name>
    <object-name>Cabin.MapLights.Bulb</object-name>
  </effect>

