<PropertyList>
    <power-knob type="int">1</power-knob>    <!-- Collins CTL-92: 0=OFF / 1=STBY / 2=ON / 4=ALT -->
    <units type="int">2</units>              <!-- CTL-92 controls 1 or 2 units -->
    <active type="bool">false</active>       <!-- active unit: false=1 / true=2 -->
</PropertyList>
