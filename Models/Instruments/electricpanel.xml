<!-- -1 LH GEN / 0 BATT / 1 RH GEN -->
  <animation>
    <type>knob</type>
    <object-name>elec.volt.knob</object-name>
    <property>controls/electric/voltage-sel</property>
    <factor>30</factor>
    <center>
      <x-m>0.0045</x-m>
      <y-m>-0.04250</y-m>
      <z-m>0.07390</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <command>property-adjust</command>
        <property>controls/electric/voltage-sel</property>
        <factor>1</factor>
        <min>-1</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <hovered>
      <binding>
        <command>set-tooltip</command>
        <tooltip-id>voltage-sel</tooltip-id>
        <label>Voltage selector: %s</label>
        <mapping>nasal</mapping>
        <script>
          var modes = ['Battery', 'Generator Right', 'Generator Left'];
          return modes[arg[0]];
        </script>
        <property>controls/electric/voltage-sel</property>
      </binding>
    </hovered>
  </animation>

