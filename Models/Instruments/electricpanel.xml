<!--
0 OFF / 1 OFF / 2 LDG GEAR / 3 BATT TEMP / 4 SPARE
5 THRU REV / 6 W/S TEMP / 7 OVERSPEED / 8 SPARE / 9 ANNU
-->

  <animation>
    <type>knob</type>
    <object-name>elec.test.knob</object-name>
    <property>controls/electric/system-test-sw</property>
    <factor>36</factor>
    <center>
      <x-m>0.00450</x-m>
      <y-m>-0.06490</y-m>
      <z-m>0.03680</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <command>property-adjust</command>
        <property>controls/electric/system-test-sw</property>
        <factor>1</factor>
        <min>0</min>
        <max>10</max>
        <wrap>true</wrap>
      </binding>
    </action>
    <hovered>
      <binding>
        <command>set-tooltip</command>
        <tooltip-id>electric-systems-test</tooltip-id>
        <label>Electric systems test: %s</label>
        <mapping>nasal</mapping>
        <script>
          var modes = ['OFF', 'OFF', 'Landing gear', 'Battery temp (non-functional)', 'Spare', 'Thrust reversers', 'W/S temp (non-functional)', 'Overspeed', 'Spare', 'Annunciators'];
          return modes[arg[0]];
        </script>
        <property>controls/electric/system-test-sw</property>
      </binding>
    </hovered>
  </animation>

  <animation>
    <type>select</type>
    <object-name>elec.test.led</object-name>
