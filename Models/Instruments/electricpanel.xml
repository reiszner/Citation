<!-- hotspots -->

<!-- 1 BATT / 0 OFF / -1 EMER -->
  <animation>
    <type>pick</type>
    <object-name>elec.batt.sw</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <condition>
          <less-than>
            <property>controls/electric/battery-bus-switch</property>
            <value>1</value>
          </less-than>
        </condition>
        <command>property-toggle</command>
        <property>sim/sound/click</property>
      </binding>
      <binding>
        <command>property-adjust</command>
        <property>controls/electric/battery-bus-switch</property>
        <step>1</step>
        <min>-1</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <hovered>
      <binding>
        <command>set-tooltip</command>
        <tooltip-id>battery-bus-switch</tooltip-id>
        <label>Battery bus: %s</label>
        <mapping>nasal</mapping>
        <script>
          var modes = ['OFF', 'BATTERY', 'EMERGENCY'];
          return modes[arg[0]];
        </script>
        <property>controls/electric/battery-bus-switch</property>
      </binding>
    </hovered>
  </animation>

