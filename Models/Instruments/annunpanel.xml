<!-- Panel Lights (only for quality-level=0) -->
    <animation>
        <type>material</type>
        <condition>
            <equals>
                <property>/sim/rendering/shaders/quality-level</property>
                <value>0</value>
            </equals>
        </condition>
        <object-name>Ctn.panel</object-name>
        <emission>
            <red>1.0</red>
            <green>0.8</green>
            <blue>0.6</blue>
            <factor-prop>controls/lighting/panel-average-lighting</factor-prop>
        </emission>
    </animation>

