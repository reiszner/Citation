<?xml version = '1.0' encoding = 'UTF-8'?>
<PropertyList>

  <path>annunpanel.ac</path>

  <animation>
    <type>select</type>
    <object-name>HydPressOn</object-name>
    <condition>
      <or>
        <property>systems/hydraulic/valves/center/state</property>
        <equals>
          <property>controls/electric/system-test</property>
          <value>9</value>
        </equals>
      </or>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>SpeedBreakExtended</object-name>
    <condition>
      <or>
        <and>
          <property>systems/electrical/outputs/main-left/sys-speed-brake</property>
          <greater-than>
            <property>surface-positions/speedbrake-pos</property>
            <value>0.99</value>
          </greater-than>
        </and>
        <equals>
          <property>controls/electric/system-test</property>
          <value>9</value>
        </equals>
      </or>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>GenOffL</object-name>
    <condition>
      <or>
        <and>
          <property>systems/electrical/outputs/main-left/annun-genoff-left</property>
          <or>
            <not><property>controls/electric/engine[0]/generator-ready</property></not>
            <not-equals>
              <property>controls/electric/engine[0]/generator-sw</property>
              <value>1</value>
            </not-equals>
          </or>
        </and>
        <equals>
          <property>controls/electric/system-test</property>
          <value>9</value>
        </equals>
      </or>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>GenOffR</object-name>
    <condition>
      <or>
        <and>
          <property>systems/electrical/outputs/main-right/annun-genoff-right</property>
          <or>
            <not><property>controls/electric/engine[1]/generator-ready</property></not>
            <less-than>
              <property>controls/electric/engine[1]/generator-sw</property>
              <value>1</value>
            </less-than>
          </or>
        </and>
        <equals>
          <property>controls/electric/system-test</property>
          <value>9</value>
        </equals>
      </or>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>ACFail</object-name>
    <condition>
      <or>
        <and>
          <property>systems/electrical/outputs/main-left/warn-lts1</property>
          <less-than>
            <property>systems/electrical/buses/AC-115V</property>
            <value>103.5</value>
          </less-than>
        </and>
        <equals>
          <property>controls/electric/system-test</property>
          <value>9</value>
        </equals>
      </or>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>DoorNotLocked</object-name>
    <condition>
      <property>systems/electrical/outputs/main-left/warn-lts1</property>
      <or>
        <greater-than>
          <property>controls/cabin-door/position-norm</property>
          <value>0</value>
        </greater-than>
        <greater-than>
          <property>controls/baggage-door-front-left/position-norm</property>
          <value>0</value>
        </greater-than>
        <greater-than>
          <property>controls/baggage-door-front-right/position-norm</property>
          <value>0</value>
        </greater-than>
        <greater-than>
          <property>controls/baggage-door-aft/position-norm</property>
          <value>0</value>
        </greater-than>
        <equals>
          <property>controls/electric/system-test</property>
          <value>9</value>
        </equals>
      </or>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>FuelBoostOnL</object-name>
    <condition>
      <or>
        <property>controls/fuel/tank[0]/boost-pump</property>
        <equals>
          <property>controls/electric/system-test</property>
          <value>9</value>
        </equals>
      </or>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>FuelBoostOnR</object-name>
    <condition>
      <or>
        <property>controls/fuel/tank[1]/boost-pump</property>
        <equals>
          <property>controls/electric/system-test</property>
          <value>9</value>
        </equals>
      </or>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>AntiSkidInop</object-name>
    <condition>
      <or>
        <and>
          <property>systems/electrical/outputs/main-left/sys-skid-ctrl</property>
          <not><property>controls/gear/antiskid</property></not>
        </and>
        <equals>
          <property>controls/electric/system-test</property>
          <value>9</value>
        </equals>
      </or>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>HydPressLoL</object-name>
    <condition>
      <or>
        <and>
          <less-than>
            <property>systems/hydraulic/supplier/pump[0]/pressure-psi</property>
            <value>45</value>
          </less-than>
          <property>systems/electrical/outputs/main-left/warn-lts1</property>
        </and>
        <equals>
          <property>controls/electric/system-test</property>
          <value>9</value>
        </equals>
      </or>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>HydPressLoR</object-name>
    <condition>
      <or>
        <and>
          <less-than>
            <property>systems/hydraulic/supplier/pump[1]/pressure-psi</property>
            <value>45</value>
          </less-than>
          <property>systems/electrical/outputs/main-right-xover/warn-lts2</property>
        </and>
        <equals>
          <property>controls/electric/system-test</property>
          <value>9</value>
        </equals>
      </or>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>OilPressLoL</object-name>
    <condition>
      <or>
        <and>
          <less-than>
            <property>engines/engine[0]/oil-pressure-psi</property>
            <value>35</value>
          </less-than>
          <property>systems/electrical/outputs/main-left/warn-lts1</property>
        </and>
        <equals>
          <property>controls/electric/system-test</property>
          <value>9</value>
        </equals>
      </or>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>OilPressLoR</object-name>
    <condition>
      <or>
        <and>
          <less-than>
            <property>engines/engine[1]/oil-pressure-psi</property>
            <value>35</value>
          </less-than>
          <property>systems/electrical/outputs/main-right-xover/warn-lts2</property>
        </and>
        <equals>
          <property>controls/electric/system-test</property>
          <value>9</value>
        </equals>
      </or>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>FuelPressLoL</object-name>
    <condition>
      <or>
        <and>
          <less-than>
            <property>engines/engine/turbine</property>
            <value>30</value>
          </less-than>
          <not><property>controls/fuel/tank[0]/boost-pump</property></not>
          <property>systems/electrical/outputs/main-left/warn-lts1</property>
        </and>
        <equals>
          <property>controls/electric/system-test</property>
          <value>9</value>
        </equals>
      </or>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>FuelPressLoR</object-name>
    <condition>
      <or>
        <and>
          <less-than>
            <property>engines/engine[1]/turbine</property>
            <value>30</value>
          </less-than>
          <not><property>controls/fuel/tank[1]/boost-pump</property></not>
          <property>systems/electrical/outputs/main-right-xover/warn-lts2</property>
        </and>
        <equals>
          <property>controls/electric/system-test</property>
          <value>9</value>
        </equals>
      </or>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>FuelLevelLoL</object-name>
    <condition>
      <or>
        <and>
          <less-than>
            <property>consumables/fuel/tank[0]/level-lbs</property>
            <value>190</value>
          </less-than>
          <property>systems/electrical/outputs/main-left/warn-lts1</property>
        </and>
        <equals>
          <property>controls/electric/system-test</property>
          <value>9</value>
        </equals>
      </or>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>FuelLevelLoR</object-name>
    <condition>
      <or>
        <and>
          <less-than>
            <property>consumables/fuel/tank[1]/level-lbs</property>
            <value>190</value>
          </less-than>
          <property>systems/electrical/outputs/main-right-xover/warn-lts2</property>
        </and>
        <equals>
          <property>controls/electric/system-test</property>
          <value>9</value>
        </equals>
      </or>
    </condition>
  </animation>

