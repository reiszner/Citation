        <action>
            <binding>
                <command>property-adjust</command>
                <property>sim/sound/click</property>
            </binding>
            <repeatable>false</repeatable>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/audiopanel/marker</property>
                <factor>-1</factor>
                <min>0</min>
                <max>2</max>
                <wrap>0</wrap>
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>audiopanel-marker</tooltip-id>
                <label>Marker: %s</label>
                <mapping>nasal</mapping>
                <script>(arg[0] == 0) ? "Speaker" : (arg[0]==2) ? "Headset" : "Off"</script>
                <property>instrumentation/audiopanel/marker</property>
