<?xml version='1.0' encoding='utf-8'?>
<PropertyList>

    <path>audiopnl.ac</path>

    <animation>
        <type>knob</type>
        <object-name>AudioPnl.volume</object-name>
        <property>instrumentation/audiopanel/volume-master</property>
        <factor>270</factor>
        <center>
            <x-m>0</x-m>
            <y-m>-0.03133</y-m>
            <z-m>0.008555</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/audiopanel/volume-master</property>
                <factor>0.05</factor>
                <min>0.0</min>
                <max>1.0</max>
                <wrap>false</wrap>
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>audiopanel-volume-master</tooltip-id>
                <label>Audio Panel Volume: %d%%</label>
                <property>instrumentation/audiopanel/volume-master</property>
                <mapping>percent</mapping>
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>AudioPnl.marker</object-name>
        <property>instrumentation/audiopanel/marker</property>
        <drag-direction>vertical</drag-direction>
        <drag-scale-px>30</drag-scale-px>
        <factor>15</factor>
        <center>
            <x-m>0.00425</x-m>
            <y-m>0.0</y-m>
            <z-m>0.01710</z-m>
