<?xml version='1.0' encoding='utf-8'?>
<PropertyList>

    <path>dme40.ac</path>

  <animation>
    <type>select</type>
    <object-name>DME.invalid</object-name>
    <condition>
      <and>
        <property alias="../../../../operable"/>
        <property alias="../../../../switch-mode"/>
        <not><property alias="../../../../../in-range"/></not>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>DME.dot</object-name>
    <object-name>DME.NM.1</object-name>
    <object-name>DME.NM1</object-name>
    <object-name>DME.NM10</object-name>
    <object-name>DME.NM100</object-name>
    <condition>
      <and>
        <property alias="../../../../operable"/>
        <property alias="../../../../switch-mode"/>
        <property alias="../../../../in-range"/>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>DME.NM10</object-name>
    <condition>
      <greater-than>
        <property alias="../../../../distance-nm"/>
        <value>99</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>DME.NM100</object-name>
    <condition>
      <greater-than>
        <property alias="../../../../distance-nm"/>
        <value>999</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>DME.min1</object-name>
    <object-name>DME.min10</object-name>
    <object-name>DME.min100</object-name>
    <condition>
      <and>
        <property alias="../../../../operable"/>
        <equals>
          <property alias="../../../../../switch-mode"/>
          <value>1</value>
        </equals>
        <property alias="../../../../in-range"/>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>DME.min10</object-name>
    <condition>
      <greater-than>
        <property alias="../../../../time-min"/>
        <value>9</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>DME.min100</object-name>
    <condition>
      <greater-than>
        <property alias="../../../../time-min"/>
        <value>99</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>DME.kt1</object-name>
    <object-name>DME.kt10</object-name>
    <object-name>DME.kt100</object-name>
    <condition>
      <and>
        <property alias="../../../../operable"/>
        <equals>
          <property alias="../../../../../switch-mode"/>
          <value>2</value>
        </equals>
        <property alias="../../../../in-range"/>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>DME.kt10</object-name>
    <condition>
      <greater-than>
        <property alias="../../../../ground-speed-kt"/>
        <value>9</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>DME.kt100</object-name>
    <condition>
      <greater-than>
        <property alias="../../../../ground-speed-kt"/>
        <value>99</value>
      </greater-than>
    </condition>
  </animation>

  <!-- distance -->
  <animation>
    <type>textranslate</type>
    <object-name>DME.NM100</object-name>
    <property alias="../../distance-nm"/>
    <factor>0.0001</factor>
    <step>1000</step>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>DME.NM10</object-name>
    <property alias="../../distance-nm"/>
    <factor>0.001</factor>
    <step>100</step>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>DME.NM1</object-name>
    <property alias="../../distance-nm"/>
    <factor>0.01</factor>
    <step>10</step>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>DME.NM.1</object-name>
    <property alias="../../distance-nm"/>
    <factor>0.1</factor>
    <step>1</step>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <!-- minutes -->
  <animation>
    <type>textranslate</type>
    <object-name>DME.min100</object-name>
    <property alias="../../time-min"/>
    <factor>0.001</factor>
    <step>100</step>
    <bias>0.5</bias>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>DME.min10</object-name>
    <property alias="../../time-min"/>
    <factor>0.01</factor>
    <step>10</step>
    <bias>0.5</bias>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>DME.min1</object-name>
    <property alias="../../time-min"/>
    <factor>0.1</factor>
    <step>1</step>
    <bias>0.5</bias>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <!-- groundspeed -->
  <animation>
    <type>textranslate</type>
    <object-name>DME.kt100</object-name>
    <property alias="../../ground-speed-kt"/>
    <factor>0.001</factor>
    <step>100</step>
    <bias>0.5</bias>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>DME.kt10</object-name>
    <property alias="../../ground-speed-kt"/>
    <factor>0.01</factor>
    <step>10</step>
    <bias>0.5</bias>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>DME.kt1</object-name>
    <property alias="../../ground-speed-kt"/>
    <factor>0.1</factor>
    <step>1</step>
    <bias>0.5</bias>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>knob</type>
    <object-name>DME.nav.knob</object-name>
    <property alias="../../switch-nav"/>
    <min>0</min>
    <max>2</max>
    <interpolation>
      <entry><ind>0</ind><dep>0</dep></entry>
      <entry><ind>1</ind><dep>30</dep></entry>
      <entry><ind>2</ind><dep>60</dep></entry>
    </interpolation>
    <center>
      <x-m>0.0050</x-m>
      <y-m>-0.026</y-m>
      <z-m>-0.026</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <command>property-adjust</command>
        <property alias="../../../../switch-nav"/>
        <factor>1</factor>
        <min>0</min>
        <max>2</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <hovered>
      <binding>
        <command>set-tooltip</command>
        <tooltip-id>DME-source</tooltip-id>
        <label>DME source: %s</label>
        <mapping>nasal</mapping>
        <script>
          var modes = ['NAV1', 'HOLD (inop)', 'NAV2'];
          return modes[arg[0]];
        </script>
        <property alias="../../../../switch-nav"/>
      </binding>
    </hovered>
  </animation>

  <animation>
    <type>knob</type>
    <object-name>DME.mode.knob</object-name>
    <property alias="../../switch-mode"/>
    <min>0</min>
    <max>2</max>
    <interpolation>
      <entry><ind>0</ind><dep>0</dep></entry>
      <entry><ind>1</ind><dep>30</dep></entry>
      <entry><ind>2</ind><dep>60</dep></entry>
    </interpolation>
    <center>
      <x-m>0.0050</x-m>
      <y-m>0.026</y-m>
      <z-m>-0.026</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <command>property-adjust</command>
        <property alias="../../../../switch-mode"/>
        <factor>1</factor>
        <min>0</min>
        <max>2</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <hovered>
      <binding>
        <command>set-tooltip</command>
        <tooltip-id>DME-mode</tooltip-id>
        <label>DME mode: %s</label>
        <mapping>nasal</mapping>
        <script>
          var modes = ['OFF', 'minutes', 'Kts'];
          return modes[arg[0]];
        </script>
        <property alias="../../../../switch-mode"/>
      </binding>
    </hovered>
  </animation>

  <animation>
    <type>knob</type>
    <object-name>DME.dim.knob</object-name>
    <property alias="../../dim"/>
    <factor>300</factor>
    <center>
      <x-m>0.0050</x-m>
      <y-m>-0.0280</y-m>
      <z-m>0.0280</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <command>property-adjust</command>
        <property alias="../../../../dim"/>
        <factor>0.04</factor>
        <min>0</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <hovered>
      <binding>
        <command>set-tooltip</command>
        <tooltip-id>dme-dim</tooltip-id>
        <label>Dimming: %d%%</label>
        <mapping>percent</mapping>
        <property alias="../../../../dim"/>
      </binding>
    </hovered>
  </animation>

<!-- Panel Lights (only for quality-level=0) -->
    <animation>
        <type>material</type>
        <condition>
            <equals>
                <property>/sim/rendering/shaders/quality-level</property>
                <value>0</value>
            </equals>
        </condition>
        <object-name>DME.bezel</object-name>
        <object-name>DME.dim.knob</object-name>
        <object-name>DME.mode.knob</object-name>
        <object-name>DME.nav.knob</object-name>
        <object-name>DME.test</object-name>
        <emission>
            <red>1.0</red>
            <green>0.8</green>
            <blue>0.6</blue>
            <factor-prop>controls/lighting/panel-average-lighting</factor-prop>
        </emission>
    </animation>

<!-- dimable lights -->
    <animation>
        <type>material</type>
        <object-name>DME.invalid</object-name>
        <object-name>DME.dot</object-name>
        <object-name>DME.NM.1</object-name>
        <object-name>DME.NM1</object-name>
        <object-name>DME.NM10</object-name>
        <object-name>DME.NM100</object-name>
        <object-name>DME.kt1</object-name>
        <object-name>DME.kt10</object-name>
        <object-name>DME.kt100</object-name>
        <object-name>DME.min1</object-name>
        <object-name>DME.min10</object-name>
        <object-name>DME.min100</object-name>
        <emission>
            <red>1.0</red>
            <green>1.0</green>
            <blue>1.0</blue>
            <factor-prop alias="../../../dim" />
        </emission>
    </animation>

    <effect>
        <inherits-from>/Models/Effects/flightdeck-glass</inherits-from>
        <object-name>DME.glass</object-name>
    </effect>

